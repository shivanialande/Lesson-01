pipeline{
        agent any
                  stages{
                         stage ('one'){
                                       steps{
                                             echo 'Hi this is Shivani'
                                       }
                          }
                          stage ('two'){
                                        steps{
                                              input ('Do you want to proceed?')
                                        }
                          }
                          stage ('three'){
                                         when{
                                              not {
                                                   branch "master"
                                                  }
                                          }
                                          steps{
                                                echo 'Hello'
                                          }
                          }                
                           stage ('four'){
                                         steps{
                                               echo 'Welcome to pipeline demo'
                                               }
                           }                    
                  }
          
}          
